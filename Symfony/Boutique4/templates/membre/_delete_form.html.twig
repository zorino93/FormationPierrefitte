<form method="post" action="{{ path('membre_delete', {'idMembre': membre.idMembre}) }}" onsubmit="return confirm('Are you sure you want to delete this item?');">
    <input type="hidden" name="_method" value="DELETE">
    <input type="hidden" name="_token" value="{{ csrf_token('delete' ~ membre.idMembre) }}">
    <button class="btn">Delete</button>
</form>
